<Window x:Class="Program.view.addOneFlashcard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Program.view"
        mc:Ignorable="d"
        Title="addOneFlashcard" Height="161" Width="401">
    <StackPanel>
        <TextBlock>Qestion</TextBlock>
        <TextBox x:Name="Qes"></TextBox>
        <TextBlock>Answer</TextBlock>
        <TextBox x:Name="Ans"></TextBox>
        <Button Click="addFlashcard"> add </Button>
    </StackPanel>
</Window>
