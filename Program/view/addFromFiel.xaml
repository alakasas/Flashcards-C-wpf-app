<Window x:Class="Program.view.addFromFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Program.view"
        mc:Ignorable="d"
        Title="addFromFile" Height="128" Width="331">


    <Grid ShowGridLines="True">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="210"/>

            <ColumnDefinition Width="*"/>

        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="47"/>
            <RowDefinition Height="*"/>

        </Grid.RowDefinitions>
        <Button Grid.Column="1" Margin="10,10,10,10" Click="ExploreWindow">explore</Button>
        <TextBlock HorizontalAlignment="Left" Height="37" Margin="10,10,0,0" TextWrapping="Wrap" Width="208" RenderTransformOrigin="0.503,0.532" Text="Zadaj cestu k súboru" FontWeight="Bold" FontSize="20"/>
        <TextBox Margin="10,7,10,6" Height="25" Grid.Row="1" Grid.ColumnSpan="2"  x:Name="textbox" KeyDown="pressEnter"  BorderBrush="DarkMagenta"/>
    </Grid>

</Window>
